#include<stdio.h>

int main()
{
    int n, num;
    printf("Enter number: ");
    scanf("%d",&n);
    num=factorial(n);
    printf("\nFactorial number: %d",num);
}
    factorial(int n)
    {
        int i;
        long fact=1;
        for(i=1;i<=n;i++)
        fact*=i;
        return fact;
        
    }
